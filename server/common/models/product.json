{
  "name": "Product",
  "plural": "products",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true
    },
    "inStock": {
      "type": "number",
      "required": true
    },
    "createAt": {
      "type": "date",
      "required": true
    },
    "image": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "category": {
      "type": "belongsTo",
      "model": "Category",
      "foreignKey": "categoryId"
    },
    "orders": {
      "type": "hasMany",
      "model": "Order",
      "foreignKey": "productId",
      "through": "OrderItem"
    }
  },
  "acls": [],
  "methods": {}
}
