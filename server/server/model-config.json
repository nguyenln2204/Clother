{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../node_modules/loopback-ds-timestamp-mixin",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "ClotherMongo",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "AccessToken": {
    "dataSource": "ClotherMongo",
    "public": false
  },
  "ACL": {
    "dataSource": "ClotherMongo",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "ClotherMongo",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "ClotherMongo",
    "public": false
  },
  "Category": {
    "dataSource": "ClotherMongo",
    "public": true
  },
  "Account": {
    "dataSource": "ClotherMongo",
    "public": true
  },
  "Review": {
    "dataSource": "ClotherMongo",
    "public": true
  },
  "Order": {
    "dataSource": "ClotherMongo",
    "public": true
  },
  "OrderItem": {
    "dataSource": "ClotherMongo",
    "public": true
  },
  "SuperProduct": {
    "dataSource": "ClotherMongo",
    "public": true
  },
  "CartItem": {
    "dataSource": "ClotherMongo",
    "public": true
  },
  "Cart": {
    "dataSource": "ClotherMongo",
    "public": true
  },
  "WishItem": {
    "dataSource": "ClotherMongo",
    "public": true
  },
  "Report": {
    "dataSource": "ClotherMongo",
    "public": false
  },
  "SupProduct": {
    "dataSource": "ClotherMongo",
    "public": true
  }
}
